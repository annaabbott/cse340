<header id="top-header">
  <span class="siteName">
    <a href="/" title="Return to home page">CSE Motors</a>
  </span>
  <div id="tools">
    <% if (accountData){ %>
    <span>Welcome, <%= accountData.account_firstname %>!</span>
    &nbsp;|&nbsp;
    <a title="Edit your account information" href="/account/edit"
      >Edit Account</a
    >&nbsp;|&nbsp; <% if (accountData.account_type !== 'client'){ %>
    <a title="admin" href="/inv/management">Manage</a>
    &nbsp;|&nbsp; <% } %>

    <a title="Click to log out" href="/account/logout">Log Out</a>
    <% } else { %>
    <a title="Click to log in" href="/account/login">Log In</a>
    <% } %>
  </div>
</header>
